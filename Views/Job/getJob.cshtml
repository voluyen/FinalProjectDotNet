@model List<FinalProjectDotNet.Models.EF.Job>

@foreach (var i in Model)
{
    <div class="tab_grid">
        <div class="col-sm-3 loc_1">
            <a href="#"><img src="/Images/loc2.jpg" alt=""></a>
        </div>
        <div class="col-sm-9">
            <div class="location_box1">
                <h6><a href="#">@i.title </a><span class="m_1">Posted 5 hours ago</span></h6>
                <p>
                    <span class="m_1">Salary : </span>@i.salary $
                </p>
                <p>
                    <span class="m_2">Location : </span>@i.location
                </p>
                <ul class="links_bottom">
                    <li><a href="#"><i class="fa fa-envelope-o icon_1"> </i><span class="icon_text"> Email this Job</span></a></li>
                    <li><a href="#"><i class="fa fa-eye icon_1"> </i> <span class="icon_text">View full Job Description</span></a></li>
                    <li class="last"><a href="@Url.Action("getDail","Job", new(i.id))"><span class="icon_text">More <i class="fa fa-caret-right icon_1"> </i></span></a></li>
                </ul>
            </div>
        </div>
        <div class="clearfix"> </div>
    </div>
}
