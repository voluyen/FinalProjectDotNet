@model List<FinalProjectDotNet.Models.EF.Job>
@{
    ViewBag.Title = "getDetail";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}
<!-- banner -->
<div class="inner_page_agile">
    <h3>Jobs</h3>
    <p>Add Some Short Description</p>

</div>
<!--//banner -->
@foreach (var i in Model)
{
    <div class="services-breadcrumb_w3layouts">
        <div class="inner_breadcrumb">

            <ul class="short_w3ls" _w3ls>
                <li>@Html.ActionLink("Trang chủ", "Home", "Default", new { area = "" }, new { @class = "nav-link" })<span>|</span></li>
                <li>@Html.ActionLink("Việc làm", "Jobs", "Default", new { area = "" }, new { @class = "nav-link" })<span>|</span></li>
                <li>@i.title</li>
            </ul>
        </div>
    </div>
    <div class="inner_content_info_agileits text-center">
        <div class="container">
            <div class="tittle_head_w3ls">
                <h3 class="tittle">@i.title</h3>
            </div>
            <div class="inner_sec_grids_info_w3ls">
                <div class="job_info_left">
                    <div class="single-left1">
                        <h3>Địa điểm</h3>
                        <p>@i.VietnamProvince.ProvinceName</p>
                        <h3>Mức lương</h3>
                        <p>@i.salary $</p>
                        <h3>Mô tả công việc</h3>
                        <p>@i.description</p>
                        <h3>Yêu cầu ứng viên</h3>
                        <p>@i.requirements</p>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>

}

