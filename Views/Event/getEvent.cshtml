@model List<FinalProjectDotNet.Models.EF.Event>



<div class="inner_sec_grids_info_w3ls">
    @foreach (var i in Model)
    {
        <div class="col-md-4 blog-grid one">
            <a href="@i.link"><img src="/Images/e6.jpg" alt=""></a>
            <div class="events_info">
                <h5>@i.date</h5>
                <h4><a href="@i.link">@i.title</a></h4>
                <p>@i.description</p>

            </div>
        </div>
    }

    <div class="clearfix"></div>

</div>